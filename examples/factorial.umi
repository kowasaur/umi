Int !(Int n, Void _) {
    if n == 0 { 1 }
    else { n * (n - 1)! }
}

Void main() {
    print(0!)  #         1
    print(1!)  #         1
    print(2!)  #         2
    print(3!)  #         6
    print(4!)  #        24
    print(5!)  #       120
    print(10!) #   3628800
    print(12!) # 479001600
}

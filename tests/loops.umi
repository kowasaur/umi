# Print the numbers from one to ten (inclusive)
Void main() {
    mut Int i = 0
    while i < 10 {
        i = i + 1
        print(i)
    }

    print("")
    while true {
        if i == 3 {
            print("3 sucks")
            i = i - 1
            continue
        }
        print(i)
        if i == 1 { break }
        i = i - 1
    }
}

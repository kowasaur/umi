class Person {
    String name
    Int age

    Person(String name, Int a) {
        this.name = name
        age = a
        print("Making " + name + " caused a side effect")
    }

    Void introduce() {
        consoleWrite("Hello I am " + name + ", ")
        print(this.age)
    }

    # Returns the Person's age after `years` years
    Int ageAfterYears(Int years) { age + years }
}


il void print(bool) "call void [mscorlib]System.Console::WriteLine(bool)"
# Since these operations are bitwise, I'm thinking to 
# use the same symbols for booleans and integers
il bool &(bool, bool) "and"
il bool |(bool, bool) "or"

void main() {
    print(true & true)
    print(true & false)
    print(false & true)
    print(false & false)

    print(true | true)
    print(true | false)
    print(false | true)
    print(false | false)
}

il void print(string) "call void [mscorlib]System.Console::WriteLine(string)"
il void print(int32) "call void [mscorlib]System.Console::WriteLine(int32)"
il void write(string) "call void [mscorlib]System.Console::Write(string)"
il string +(string, string) "call string string::Concat(string, string)"

string createHello(string name) {
    # Implicit return
    "Hello, " + name
}

void greet(string name, int32 age) {
    string message = createHello(name)
    write(message)
    message = " aged "
    write(message)
    print(age)
}

# Example of function overloading
void greet(int32 age) {
    greet("Tim", age)
}

void main() {
    greet("Joe", 3333)
    greet(52)
}

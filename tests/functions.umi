String createHello(String name) {
    # Implicit return
    "Hello, " + name
}

Void greet(String name, Int age) {
    String message = createHello(name)
    consoleWrite(message)
    message = " aged "
    consoleWrite(message)
    print(age)
}

# Example of function overloading
Void greet(Int age) {
    greet("Tim", age)
}

Void main() {
    greet("Joe", 3333)
    greet(52)
}
